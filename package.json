{
  "name": "@glensorbo/requestlogger",
  "version": "1.1.2",
  "description": "Simple http logger middleware for expressjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "tscheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts --cache --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "build": "tsup",
    "codeCheck": "npm run tscheck && npm run lint && npm run prettier",
    "release": "npm run build && changeset publish",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glensorbo/requestLogger.git"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/express": "^5.0.0",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.30.0",
    "husky": "^9.1.6",
    "prettier": "3.4.2",
    "prettier-plugin-sort-imports": "^1.8.6",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "filesize": "^10.1.6"
  },
  "keywords": [
    "http",
    "express",
    "expressjs",
    "logger",
    "middleware",
    "request"
  ],
  "author": "glensorbo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/glensorbo/requestLogger/issues"
  },
  "homepage": "https://github.com/glensorbo/requestLogger#readme"
}
